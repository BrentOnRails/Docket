        <li data-target="#carousel-example-generic" data-slide-to="1" class=""></li>
        <li data-target="#carousel-example-generic" data-slide-to="2" class=""></li>
      </ol>
      <div class="carousel-inner">
        <div class="item active">
          <%= cl_image_tag("people.jpg", :width => 2000, :height => 750, :crop => :fill) %>
          <div class="carousel-caption">
            <a class="btn btn-primary" href="/users/sign_in">Sign In</a>
            <a class="btn btn-primary" href="/users/sign_up">Sign Up</a>
          </div>
        </div>
        <div class="item">
        <%= cl_image_tag("proper-handshake.jpg", :width => 2000, :height => 750, :crop => :fill )%>
        <div class="carousel-caption">
          <a class="btn btn-primary" href="/users/sign_in">Sign In</a>
          <a class="btn btn-primary" href="/users/sign_up">Sign Up</a>
        </div>
        </div>

        <div class="item">
        <%= cl_image_tag("docket.png", :width => 2000, :height => 750, :crop => :fill) %>
        <div class="carousel-caption">
          <a class="btn btn-primary" href="/users/sign_in">Sign In</a>
          <a class="btn btn-primary" href="/users/sign_up">Sign Up</a>
        </div>
        </div>

      </div>
      <a class="left carousel-control" href="#carousel-example-generic" data-slide="prev">
