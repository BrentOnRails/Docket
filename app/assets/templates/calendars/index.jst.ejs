<div class="well well-lrg"><h2> Docket - Calendars </h2>




<a href="/#calendars/new" class="btn btn-inverse"><span class="glyphicon glyphicon-calendar"></span>  </a>
<% if (calendars.length > 0) { %>
  <% calendars.each(function (calendar) { %>
  <div class="btn-group">

    <button type="button" class="btn btn-inverse">
    <span class="glyphicon glyphicon-calendar"></span>
     <%= calendar.escape("title") %></button>

    <button type="button" class="btn btn-inverse dropdown-toggle" data-toggle="dropdown">
      <span class="caret"></span>
      <span class="sr-only">Toggle Dropdown</span>
    </button>
    <ul class="dropdown-menu" role="menu">
      <li><a href="#">Toggle</a></li>
      <li><a href="#calendars/<%= calendar.escape("id") %>/edit">Edit</a></li>
      <li><a href="#">Share</a></li>
      <li class="divider"></li>
      <li><a data-id="<%= calendar.escape("id")%>" class="delete-calendar" href="#">Delete</a></li>
    </ul>
  </div>
  <% }) %>


</div>


<a href="/#entries/new" class="btn btn-primary"> Add New Event </a><br><br>




	<% var dates = [] %>
  <% entries.each(function(entry) { %>

	  <% if(_.indexOf(dates, entry.escape("date"), true) == -1) { %>
			<h4><%= entry.escape("date") %> </h4><br>
		<% } %>
    <div class="panel-group" id="accordion">
      <div class="panel panel-inverse">
      <div class="panel-heading"><a data-toggle="collapse" data-parent="accordion" href="#collapse<%= entry.id %>"><%= entry.escape("title") %></a></div>

      <div id="collapse<%= entry.id%>" class="panel-collapse collapse in">
      <div class="panel-body">
        Notes: <%= entry.escape("notes") %> <Br>
		    Calendar: <%= calendars.get(entry.escape("calendar_id")).escape("title") %> <br>
		    <% dates.push(entry.escape("date")) %>

        <a href="#entries/<%= entry.escape('id')%>/edit" >Edit</a> -
        <a data-id="<%= entry.escape("id")%>" data-calendar-id ="<%= entry.escape("calendar_id") %>" class="delete-event" href="#">Delete</a></li>
      </div>
      </div>
    </div>
 <% }) %>

<div>


<% } else { %>
  You have no calendars <br>
<% } %>


